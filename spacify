public class Spacify {
    public static String spacify(String str){
    String temp = "";
      for(int i = 0; i< str.length()-1; i++)
      {
        temp+= str.charAt(i);
        temp+= " ";
      }
      temp += str.charAt(str.length()-1);
      str = temp;
        return str; //show me the code!
    }
}
